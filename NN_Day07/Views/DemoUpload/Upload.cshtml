

<form enctype="multipart/form-data" asp-action="UploadSingleFile">
    <h2>Upload Single File</h2>
    <input type="file" name="myfile" required>
    <button>Upload</button>
</form>

<form enctype="multipart/form-data" asp-action="UploadMultipleFile">
    <h2>Upload Multiple File</h2>
    <input type="file" name="mymultiple" multiple/>
    <button>Upload</button>
</form>